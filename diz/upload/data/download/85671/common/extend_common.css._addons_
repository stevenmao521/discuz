.wp { width: 1170px; }
.ct2_a .mn { width: 1010px; }
.ct3_a .mn { width:780px;}
#asl_hd { height: 61px; background: #2f549f; }
#asl_logo { float: left; padding-left: 15px; padding-top: 11px; }
#asl_logo img { width: 200px; }
#asl_nv { float: left; width: 530px; height: 60px; overflow: hidden; }
#asl_nv li { float: left; }
#asl_nv li a { display: block; height: 60px; line-height: 60px; color: rgba(255,255,255,.7); padding: 0 25px 0 30px; font-size: 14px; }
#asl_nv li a:hover, #asl_nv li.a a { color: #fff; text-decoration: none; }
#asl_nv li a span { display: none; }
#asl_login li { float: left; }
#asl_login li a { display: block; height: 60px; line-height: 60px; color: rgba(255,255,255,.7); padding: 0 10px; }
#asl_login li a:hover { color: #fff; text-decoration: none; }
#asl_user a { display: block; color: rgba(255,255,255,.7); float: right; }
#asl_user a.admincp { margin-top: 20px; margin-right: 10px; }
#asl_user a.pice { margin-top: 20px; margin-right: 10px; background: #FF5722; color: #fff; padding: 0 3px; }
#asl_user a img { border-radius: 50%; margin-top: 6px; }
#setup_menu { width: 174px; background: #fff; border: 1px solid #d2d2d2; box-shadow: 0 2px 4px rgba(0,0,0,.12); padding: 5px 0; }
#setup_menu a { text-align: center; }
#setup_menu a:hover { background: #f2f2f2; color: #000; }
#asl_sup { height: 50px; line-height: 50px; background: #fff; border-radius: 2px; margin-bottom: 15px; box-shadow: 0 1px 2px 0 rgba(0,0,0,.05); }
#asl_sup { }
#asl_sup ul li { float: left; display: inline-block; height: 50px; line-height: 50px; }
#asl_sup ul li a { float: left; padding: 0 20px; font-size: 14px; }
#asl_sup ul li a:hover { color: #2f549f; transition: all .3s; text-decoration: none; }
.asl_sup_post span, .asl_sup_post a { float: left; }
.asl_sup_post a { display: block; height: 38px; line-height: 38px; border: 1px solid #C9C9C9; background: #fff; color: #555; padding: 0 18px; font-size: 14px; border-radius: 2px; cursor: pointer; margin-top: 5px; }
.asl_sup_post a:hover { border-color: #2f549f; text-decoration: none; }
.asl_sup_post span:hover { background: url({IMGDIR}/dd_hover.png) !important; }
.asl_sup_post span a { display: block; width: 19px; height: 19px; }
#diy-tg { margin-top: 16px; margin-right: 10px; }
#diy-tg_menu { margin-left: -17px; margin-top: -5px; }
#diy-tg_menu a { color: #369 !important; }
#asl_foot { text-align: center; padding: 20px 0 30px; border-top: 1px solid #e2e2e2; }
#asl_foot p { height: 30px; line-height: 30px; }
#asl_foot p a { color: #333; }
#pm_ntc.new:hover { background-image: url({IMGDIR}/new_pm.gif) !important; }
.ct2 .mn { width: 776px; }
.ct2 .sd { width: 380px; }
#asl_top_post { margin-bottom: 15px; border-radius: 2px; background: #fff; box-shadow: 0 1px 2px 0 rgba(0,0,0,.05) }
#asl_top_post a { display: block; width: 100%; height: 38px; line-height: 38px; padding: 0 18px; background: #FF5722; color: #fff; white-space: nowrap; text-align: center; font-size: 14px; border: none; border-radius: 2px; cursor: pointer; }
#asl_top_post a:hover { opacity: .8; filter: alpha(opacity=80); color: #fff; text-decoration: none; }
.asl_hook span { display:none}
#asl_login .fastlg_fm { float: left !important; border-right: 0 !important; margin-top: 20px; margin-right: 0 !important; }
#asl_login .fastlg_fm .hm { display: none }